{
    "name": "host",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "host/src",
    "projectType": "application",
    "tags": [],
    "targets": {
      "build": {
        "executor": "@nx/rspack:rspack",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "development",
        "options": {
          "target": "web",
          "outputPath": "dist/host",
          "index": "host/src/index.html",
          "main": "host/src/main.tsx",
          "tsConfig": "host/tsconfig.app.json",
          "rspackConfig": "host/rspack.config.ts",
          "assets": [
            "host/src/favicon.ico",
            "host/src/assets"
          ]
        },
        "configurations": {
          "development": {
            "mode": "development"
          },
          "production": {
            "mode": "production",
            "optimization": true,
            "sourceMap": false
          }
        }
      },
      "serve": {
        "executor": "@nx/rspack:dev-server",
        "dependsOn": [
          "^build"
        ],
        "options": {
          "buildTarget": "host:build:development",
          "port": 4200
        },
        "configurations": {
          "development": {},
          "production": {
            "buildTarget": "host:build:production"
          }
        }
      },
      "serve-static": {
        "executor": "@nx/web:file-server",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "host:build",
          "spa": true
        }
      }
    }
  }